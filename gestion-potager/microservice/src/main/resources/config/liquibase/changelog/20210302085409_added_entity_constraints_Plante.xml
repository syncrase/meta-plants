<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Plante.
    -->
    <changeSet id="20210302085409-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cycle_de_vie_id"
                                 baseTableName="plante"
                                 constraintName="fk_plante_cycle_de_vie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cycle_de_vie"/>

        <addForeignKeyConstraint baseColumnNames="classification_id"
                                 baseTableName="plante"
                                 constraintName="fk_plante_classification_id"
                                 referencedColumnNames="id"
                                 referencedTableName="classification"/>

        <addForeignKeyConstraint baseColumnNames="plante_id"
                                 baseTableName="plante_noms_vernaculaires"
                                 constraintName="fk_plante_noms_vernaculaires_plante_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plante"/>

        <addForeignKeyConstraint baseColumnNames="noms_vernaculaires_id"
                                 baseTableName="plante_noms_vernaculaires"
                                 constraintName="fk_plante_noms_vernaculaires_noms_vernaculaires_id"
                                 referencedColumnNames="id"
                                 referencedTableName="nom_vernaculaire"/>
        
    </changeSet>
</databaseChangeLog>
